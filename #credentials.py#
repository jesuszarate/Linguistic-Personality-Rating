consumerSecret = 'rxcO4CyJ9Dxcnw5l3kMf2zLVyCIp8NPdXNAkQr0RVN3kpZbc5f'
accessToken = '700482498946437121-4qvu9cmoZ0Nj74gFEVz1yxZJYJW6jjP'
accessSecret = '7DyM8SNNR5uPSVFrMGGrnfG0eSPWrIgMDI2AkhHriSjxY'

keyVars = ['consumerKey', 'consumerSecret', 'accessToken', 'accessSecret']
availablekeys = []

def errorOutput(lines):
    for line in lines:
        k = line.split()[0]
        availablekeys.append(k)

    missing = list(set(keyVars) - set(availablekeys))
    print 'You\'re missing the following credential keys: '
    for m in missing:
        print m
    
        errorOutput(lines)
    else:
        for line in lines:
            if len(line.split()) < 1:
                errorOutput(lines)

